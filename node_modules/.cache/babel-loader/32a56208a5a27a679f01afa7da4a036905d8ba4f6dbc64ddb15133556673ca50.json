{"ast":null,"code":"//IMPORTS\nimport { useRoute } from 'vue-router';\nimport projets from \"@/projets.js\";\n\n//COMPONENTS\nimport NavBar from '@/components/NavBar.vue';\nimport FooterCard from '@/components/FooterCard.vue';\nexport default {\n  name: 'InfoProjet',\n  components: {\n    NavBar,\n    FooterCard\n  },\n  data: () => {\n    return {\n      scroll: false\n    };\n  },\n  setup() {\n    const route = useRoute();\n    const projet = projets.find(projetName => {\n      if (projetName.name == route.params.projetName) {\n        return projetName;\n      }\n    });\n    const lien = projet.lien;\n    return {\n      projet,\n      lien\n    };\n  },\n  methods: {\n    handleScroll() {\n      if (window.scrollY > 150) {\n        this.scroll = true;\n      } else {\n        this.scroll = false;\n      }\n    },\n    scrollUp() {\n      window.scrollTo(0, 0);\n    }\n  },\n  mounted() {\n    window.addEventListener(\"scroll\", this.handleScroll);\n  },\n  unmounted() {\n    window.removeEventListener(\"scroll\", this.handleScroll);\n  }\n};","map":{"version":3,"mappings":"AA4BA;AACA,SAASA,QAAO,QAAS,YAAW;AACpC,OAAOC,OAAM,MAAO,cAAa;;AAEjC;AACA,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,UAAS,MAAO,6BAA6B;AACpD,eAAe;EACXC,IAAG,EAAI,YAAY;EACnBC,UAAU,EAAC;IACPH,MAAM;IACNC;EACJ,CAAC;EACDG,IAAI,EAAC,MAAI;IACL,OAAO;MACHC,MAAK,EAAI;IACb;EACJ,CAAC;EACDC,KAAK,GAAE;IACL,MAAMC,KAAI,GAAIT,QAAQ,EAAE;IACxB,MAAMU,MAAK,GAAIT,OAAO,CAACU,IAAI,CAAEC,UAAU,IAAG;MACxC,IAAGA,UAAU,CAACR,IAAG,IAAKK,KAAK,CAACI,MAAM,CAACD,UAAU,EAAC;QAC1C,OAAOA,UAAU;MACnB;IACJ,CAAC,CAAC;IAEF,MAAME,IAAG,GAAIJ,MAAM,CAACI,IAAI;IAExB,OAAO;MACLJ,MAAM;MACNI;IACF;EAEF,CAAC;EACDC,OAAO,EAAC;IACJC,YAAY,GAAE;MACV,IAAGC,MAAM,CAACC,OAAM,GAAI,GAAG,EAAC;QACpB,IAAI,CAACX,MAAK,GAAI,IAAI;MACtB,CAAC,MAAI;QACD,IAAI,CAACA,MAAK,GAAI,KAAK;MACvB;IACJ,CAAC;IACDY,QAAQ,GAAE;MACNF,MAAM,CAACG,QAAQ,CAAC,CAAC,EAAC,CAAC,CAAC;IACxB;EACJ,CAAC;EACDC,OAAO,GAAG;IACNJ,MAAM,CAACK,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACN,YAAY,CAAC;EACxD,CAAC;EACDO,SAAS,GAAG;IACR